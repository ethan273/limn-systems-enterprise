{
  "name": "limn-systems-enterprise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:fast": "SKIP_ENV_VALIDATION=true next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "security:audit": "npm audit && npm audit --audit-level high",
    "security:scan": "npm run lint 2>&1 | grep -i security | head -10 || echo 'No critical security issues found in linting'",
    "security:check": "npm run security:audit && npm run security:scan",
    "pre-commit": "npm run lint && npm run type-check && npm run test:ci && npm run security:audit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
